<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="110dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView17"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:background="#1F1F1F"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="28dp"
            android:layout_marginTop="96dp" app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView24"
            app:layout_constraintStart_toEndOf="@+id/logo" app:layout_constraintHorizontal_bias="0.94"
            app:layout_constraintEnd_toStartOf="@+id/imageView27" app:layout_constraintTop_toTopOf="@+id/imageView27"
            app:layout_constraintBottom_toBottomOf="@+id/imageView27"
            app:layout_constraintHorizontal_chainStyle="packed"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView27"
            app:layout_constraintStart_toEndOf="@+id/imageView24" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView28" app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28" android:layout_marginStart="32dp"
            app:layout_constraintVertical_bias="1.0"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView28"
            app:layout_constraintStart_toEndOf="@+id/imageView27" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17" android:layout_marginStart="32dp"/>
    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="20sp" app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginStart="28dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17"/>
    <ImageView
            android:layout_width="140dp"
            android:layout_height="140dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView29"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView30"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintTop_toBottomOf="@+id/logo" android:layout_marginStart="16dp"
            app:layout_constraintVertical_bias="0.574"/>
    <ImageView
            android:layout_width="140dp"
            android:layout_height="140dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView30"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView29"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29"
            app:layout_constraintTop_toTopOf="@+id/imageView29" app:layout_constraintVertical_bias="0.0"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/guideline3"/>
    <ImageView
            android:layout_width="140dp"
            android:layout_height="140dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView31"
            android:scaleType="centerCrop"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintBottom_toBottomOf="@+id/imageView29"
            app:layout_constraintStart_toEndOf="@+id/imageView30"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView32"
            app:layout_constraintTop_toTopOf="@+id/imageView29"/>
    <ImageView
            android:layout_width="140dp"
            android:layout_height="140dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView32"
            android:scaleType="centerCrop" app:layout_constraintStart_toEndOf="@+id/imageView31"
            android:layout_marginStart="16dp" app:layout_constraintBottom_toBottomOf="@+id/imageView31"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintTop_toTopOf="@+id/imageView31"
            android:layout_marginEnd="16dp"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline3"
                                          android:orientation="vertical"
                                          app:layout_constraintGuide_percent="0.50073206"/>
</android.support.constraint.ConstraintLayout>